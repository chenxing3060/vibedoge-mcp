{
  "name": "vibe-coding-lottery-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Vibe Coding Lottery System",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "start:prod": "./start.sh",
    "start:dev": "./start-dev.sh",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "echo \"MCP Server ready for testing\" && exit 0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "lottery",
    "vibe-coding"
  ],
  "author": "Vibe Coding Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}